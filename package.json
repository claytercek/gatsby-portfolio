{
  "name": "gatsby-portfolio",
  "private": true,
  "version": "1.1.0",
  "author": "Clay Tercek <code@claytercek.com>",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "gatsby": "^2.24.66",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-catch-links": "^2.3.13",
    "gatsby-plugin-emotion": "^4.3.12",
    "gatsby-plugin-ffmpeg": "^0.3.1",
    "gatsby-plugin-manifest": "^2.4.32",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.3.31",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-remark": "^2.8.36",
    "gatsby-transformer-sharp": "^2.5.16",
    "html-react-parser": "^0.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.27",
    "rehype-react": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build  --log-pages",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "run-s -n build serve",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "run-s -n lint:*",
    "lint:js": "eslint . --fix --max-warnings 8",
    "lint:style": "stylelint --fix --mw 8 \"**/*.{css,scss,sass,js,jsx}\"",
    "lint:format": "prettier --write \"**/*.{js,jsx,json,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{css,scss,sass,js,jsx}": "stylelint --fix",
    "*.{js,jsx,json,md}": "prettier --write"
  }
}
